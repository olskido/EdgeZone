{
  "name": "edgezone-signal-engine",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node --transpile-only src/server.ts",
    "dev:workers": "ts-node --transpile-only src/worker.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "start:workers": "node dist/worker.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^6.4.1",
    "bullmq": "^5.52.3",
    "dotenv": "^16.4.7",
    "fastify": "^4.29.1",
    "ioredis": "^5.5.0",
    "openai": "^6.17.0",
    "pino": "^9.6.0",
    "prisma": "^6.4.1",
    "undici": "^7.20.0",
    "ws": "^8.19.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.20.0",
    "prettier": "^3.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
